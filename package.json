{
  "name": "remote-cloud-shell",
  "displayName": "Remote - Cloud Shell",
  "description": "Use Google Cloud Shell as your development environment.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.97.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SnpM/remote-cloud-shell"
  },
  "categories": ["Other"],
  "license": "MIT",
  "publisher": "Nibs",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Remote - Cloud Shell Settings",
      "properties": {
        "remote-cloud-shell.gcloudPath": {
          "type": "string",
          "default": "",
          "description": "Manually specify the gcloud bin folder path if it is not in the system PATH. This string is prepended to the gcloud command."
        }
      }
    },
    "commands": [
      {
        "command": "remote-cloud-shell.get-command-gcloud",
        "title": "Get ssh command from Google Cloud (gcloud)",
        "category": "Remote-Cloud Shell"
      },
      {
        "command": "remote-cloud-shell.add-host-gcloud",
        "title": "Add ssh host from Google Cloud (gcloud)",
        "category": "Remote-Cloud Shell"
      },
      {
        "command": "remote-cloud-shell.update-config-gcloud",
        "title": "Update ssh config from Google Cloud (gcloud)",
        "category": "Remote-Cloud Shell"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "vsce": "^2.17.0",
    "copy-webpack-plugin": "^11.0.0",
    "vscode": "^1.97.0"
  },
  "dependencies": {
  }
}
